{
    "name": "filippo-toso/laravel-helpers",
    "description": "A couple of helpers for Laravel development",
    "license": "MIT",
    "authors": [{
        "name": "Filippo Toso",
        "email": "filippo.toso@creativepark.it"
    }],
    "require": {
        "php": "^7.0 || ^8.0 || ^9.0",
        "illuminate/support": ">=5.5",
        "laravel/helpers": "^1.1",
        "symfony/error-handler": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "FilippoToso\\LaravelHelpers\\": "src"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "FilippoToso\\LaravelHelpers\\ServiceProvider"
            ],
            "aliases": {
                "Breadcrumbs": "FilippoToso\\LaravelHelpers\\Facades\\Breadcrumbs",
                "Gravatar": "FilippoToso\\LaravelHelpers\\Facades\\Gravatar"
            }
        }
    }
}